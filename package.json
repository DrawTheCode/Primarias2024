    "@types/node": "^20.12.12",
    "@types/node-cron": "3.0.11",
    "@types/ssh2-sftp-client": "9.0.3",
    "fs-extra": "11.1.1",
    "ts-node-dev": "2.0.0",
    "ts-standard": "12.0.2",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.6",
